{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "@Components": ["src/Components"],
      "@Components/*": ["src/Components/*"],
      "@common/*": ["src/common/*"],
      "@navigators/*": ["src/navigators/*"],
      "@screens/*": ["src/screens/*"],
      "@hooks/*": ["src/hooks/*"],
      "types/*": ["./@types/*"]
    }
  }
}
